{
  "rewrites": [
    {
      "source": "/openapi-public.yaml",
      "destination": "/openapi.yaml"
    },
    {
      "source": "/ai-plugin.json",
      "destination": "/ai-plugin.json"
    },
    {
      "source": "/privacy.html",
      "destination": "/privacy.html"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
