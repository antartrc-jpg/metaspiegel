{
  "rewrites": [
    {
      "source": "/openapi.yaml",
      "destination": "/Public/openapi-public.yaml"
    },
    {
      "source": "/ai-plugin.json",
      "destination": "/Public/ai-plugin.json"
    },
    {
      "source": "/privacy.html",
      "destination": "/Public/privacy.html"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
